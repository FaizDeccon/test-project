<p id="notice"><%= notice %></p>

<% @posts.each do |post| %>
    <%= post.image %>
    <%= post.user.username %>
    <%= post.caption %>
    <%= post.like %>
<% end %>

<%= link_to 'New Post', new_post_path %>
<% if user_signed_in? %>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
<% end %>
