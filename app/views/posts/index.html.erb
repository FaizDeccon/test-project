<p id="notice"><%= notice %></p>

<% @posts.each do |post| %>
<% if post.user == current_user %>
    <% post.avatars.each do |avatar| %>
      <%= image_tag avatar.url %>
    <% end %>
    <%= post.user.username %>
    <%= post.caption %>
    <%= post.like %>
  <% end %>
<% end %>

<%= link_to 'New Post', new_post_path %>
<% if user_signed_in? %>
  <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
<% end %>
